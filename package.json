    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^2.7.12",
    "@types/node": "^18.19.7",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.7",
    "@types/sequelize": "^4.28.20",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2"
  },
  "main": "app.js",
