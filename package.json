{
  "name": "call-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=local NODE_SRV=S nodemon --exec ts-node ./bin/www.ts",
    "dev:admin": "cross-env NODE_ENV=local NODE_SRV=A nodemon --exec ts-node ./bin/www.ts",
    "build": "tsc && npm run copy-files",
