    "start:dev": "cross-env NODE_ENV=development NODE_SRV=S node dist/bin/www.ts",
    "start:prod": "cross-env NODE_ENV=production NODE_SRV=S node dist/bin/www.ts",
    "start:admin": "cross-env NODE_ENV=local NODE_SRV=A node dist/bin/www.ts",
    "start:admin_dev": "cross-env NODE_ENV=development NODE_SRV=A node dist/bin/www.ts",
    "start:admin_prod": "cross-env NODE_ENV=production NODE_SRV=A node dist/bin/www.ts"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "hide-powered-by": "^1.1.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "path": "^0.12.7",
    "sequelize": "^6.35.2",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
